{
    "containerDefinitions": [
        {
            "name": "front-end",
            "image": "public.ecr.aws/docker/library/nginx:latest",
            "cpu": 512,
            "memory": 1024,
            "links": [],
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [
                "node",
                "server.js"
            ],
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::pb-dev-env-variables/dev/front.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [],
            "dnsServers": [],
            "dnsSearchDomains": [],
            "extraHosts": [],
            "dockerSecurityOptions": [],
            "dockerLabels": {},
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "front-end",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "front-end",
                    "max-buffer-size": "25m",
                    "mode": "non-blocking"
                },
                "secretOptions": []
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "echo health || exit 1"
                ],
                "interval": 5,
                "timeout": 3,
                "retries": 2
            },
            "systemControls": [],
            "credentialSpecs": []
        }
    ],
    "family": "front-end",
    "executionRoleArn": "arn:aws:iam::211125694497:role/api-gateway-fargate-ClusterSta-ECSTaskExecutionRole-8kYmwJTFnKeS",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "tags": [
        {
            "key": "lambda:createdBy",
            "value": "SAM"
        }
    ]
}